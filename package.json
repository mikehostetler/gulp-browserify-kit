{
  "name": "gulp-browserify-kit",
  "version": "1.0.0",
  "description": "sample kit for using gulp + browserify",
  "repository": {},
  "//": [],
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "jquery-plugin": "some-jquery-plugin/dist/plugin.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
